<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<HBox xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox prefHeight="50.0" prefWidth="100.0">
         <children>
             <Label fx:id="periodFxml" styleClass="cell_big_label" text="\$period" wrapText="true" />
             <Label fx:id="nameFxml" styleClass="cell_small_label" text="\$name" wrapText="true" />
             <Label fx:id="subjectFxml" styleClass="cell_small_label" text="\$subject" wrapText="true"/>
         </children></VBox>
   </children>
</HBox>
