<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<TabPane fx:id="tabPane" tabClosingPolicy="UNAVAILABLE" tabMaxWidth="Infinity" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <tabs>
      <Tab fx:id="modules" closable="false" text="Modules">
         <content>
            <StackPane>
               <children>
                  <HBox alignment="CENTER" spacing="5.0" StackPane.alignment="TOP_LEFT">
                     <children>
                        <VBox fx:id="moduleList" minWidth="250.0" styleClass="vbox-with-border" HBox.hgrow="ALWAYS">
                           <children>
                              <StackPane fx:id="moduleListPanelPlaceholder" styleClass="empty-container" VBox.vgrow="ALWAYS" />
                           </children>
                        </VBox>
                        <VBox alignment="CENTER" styleClass="vbox-with-border" />
                     </children>
                     <StackPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </StackPane.margin>
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                  </HBox>
               </children>
            </StackPane>
         </content>
      </Tab>
      <Tab fx:id="contacts" text="Contacts">
         <content>
            <StackPane>
               <children>
                  <HBox spacing="5.0" StackPane.alignment="TOP_LEFT">
                     <children>
                        <VBox fx:id="personList" minWidth="250.0" styleClass="vbox-with-border" HBox.hgrow="ALWAYS">
                           <children>
                              <StackPane fx:id="personListPanelPlaceholder" styleClass="empty-container" />
                           </children>
                        </VBox>
                        <VBox alignment="CENTER" styleClass="vbox-with-border" />
                     </children>
                     <StackPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </StackPane.margin>
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                  </HBox>
               </children>
            </StackPane>
         </content>
      </Tab>
   </tabs>
</TabPane>
