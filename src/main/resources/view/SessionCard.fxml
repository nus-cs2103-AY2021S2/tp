<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<AnchorPane id="sessionCardPane" fx:id="sessionCardPane" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1">
    <HBox AnchorPane.leftAnchor="10">
        <VBox>
            <padding>
                <Insets bottom="5" left="5" right="5" top="5" />
            </padding>
            <Label fx:id="date" styleClass="cell_small_label" text="\$date" wrapText="true"/>
            <Label fx:id="time" styleClass="cell_small_label" text="\$time" wrapText="true"/>
            <Label fx:id="duration" styleClass="cell_small_label" text="\$duration" wrapText="true"/>
            <Label fx:id="subject" styleClass="cell_small_label" text="\$subject" wrapText="true"/>
            <Label fx:id="fee" styleClass="cell_small_label" text="\$fee" wrapText="true"/>
        </VBox>
        <VBox>
            <padding>
                <Insets bottom="5" left="15" right="5" top="5" />
            </padding>
            <Label fx:id="endDate" styleClass="cell_small_label" text="\$endDate" wrapText="true"/>
            <Label fx:id="interval" styleClass="cell_small_label" text="\$interval" wrapText="true"/>
        </VBox>
    </HBox>
    <Label fx:id="id" styleClass="cell_big_label" AnchorPane.rightAnchor="10" AnchorPane.topAnchor="10">
        <padding>
            <Insets bottom="5" left="5" right="5" top="5" />
        </padding>
    </Label>
    <Label fx:id="sessionType" styleClass="cell_big_label" AnchorPane.rightAnchor="10" AnchorPane.bottomAnchor="10">
        <padding>
            <Insets bottom="5" left="5" right="5" top="5" />
        </padding>
    </Label>
</AnchorPane>
