<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Text?>
<VBox VBox.vgrow="ALWAYS" styleClass="cardPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
        <TextField styleClass="stack-pane" fx:id="sectionTitle"
                     promptText="New Section">
        </TextField>

        <TextArea prefWidth="150" prefRowCount="1" wrapText="true" fx:id="sectionBody"
                     promptText="Enter text here..."/>
</VBox>
