<fx:root title="Important Dates!" type="javafx.stage.Stage" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <icons>
        <Image url="@/images/important_dates.png" />
    </icons>

    <scene>
        <Scene>
            <stylesheets>
                <URL value="@DarkTheme.css" />
                <URL value="@Extensions.css" />
            </stylesheets>
            <VBox alignment="CENTER">
                 <StackPane fx:id="dateListPanelPlaceholder" prefHeight="401.0" prefWidth="300.0" VBox.vgrow="ALWAYS" />
            </VBox>
        </Scene>
    </scene>
</fx:root>
