<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.ListView?>
<VBox id="tuitionCardPane" fx:id="tuitionCardPane" styleClass="tuition" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1" prefHeight="${sessionListView.minHeight}" >
    <padding>
      <Insets bottom="5" left="15" right="5" top="5" />
    </padding>
    <Label fx:id="name" textAlignment="center">
      <padding>
        <Insets top="5" bottom="5" right="5" left="17" />
      </padding>
    </Label>
    <ListView fx:id="sessionListView" />
</VBox>
