<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<?import javafx.geometry.Insets?>
<HBox id="slotPane" fx:id="slotPane" styleClass="appointment-grid-cell" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1">
    <VBox GridPane.columnIndex="0">
        <padding>
            <Insets top="8" left="10" />
        </padding>
        <HBox alignment="CENTER_LEFT">
            <Label fx:id="title" text="\$title" style="-fx-font-size: 10pt" />
        </HBox>
        <Label fx:id="time" text="\$time" style="-fx-font-size: 10pt" />
        <Label fx:id="description" style="-fx-font-size: 10pt" />
    </VBox>
</HBox>
